<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.questory.repository.SecurityMemberRepository">

    <select id="findByEmail" resultType="com.ssafy.questory.domain.Member" parameterType="string">
        SELECT *
        FROM members
        WHERE email = #{email}
    </select>

</mapper>
